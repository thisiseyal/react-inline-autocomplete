{
    "name": "react-inline-autocomplete",
    "version": "1.0.5",
    "description": "react-inline-autocomplete",
    "keywords": [
        "react",
        "component",
        "react-component",
        "inline",
        "autocomplete",
        "inline-autocomplete"
    ],
    "source": "lib/index.tsx",
    "main": "dist/index.js",
    "module": "dist/index.module.js",
    "umd:main": "dist/index.umd.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "microbundle --no-compress --globals react=React --jsx React.createElement",
        "dev": "microbundle watch --no-compress --globals react=React --jsx React.createElement",
        "deploy": "gh-pages -d example/build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thisiseyal/react-inline-autocomplete.git"
    },
    "author": "Kyu",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/thisiseyal/react-inline-autocomplete/issues"
    },
    "homepage": "https://github.com/thisiseyal/react-inline-autocomplete#readme",
    "dependencies": {
        "@types/classnames": "^2.2.10",
        "@types/react": "^18.2.7",
        "classnames": "^2.2.6",
        "ignore-case": "^0.1.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@typescript-eslint/eslint-plugin": "^4.3.0",
        "@typescript-eslint/parser": "^4.3.0",
        "eslint": "^7.10.0",
        "gh-pages": "^3.1.0",
        "husky": "^4.3.0",
        "lint-staged": "^10.4.0",
        "microbundle": "^0.12.4"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "lint-staged": {
        "lib/**/*.ts?(x)": [
            "eslint --fix",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}